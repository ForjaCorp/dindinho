{
  "name": "dindinho-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "start": "ng serve",
    "docs:check": "node ./scripts/validate-docs.mjs",
    "build": "npm run docs:check && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false",
    "test:unit": "ng test --watch=false",
    "test:e2e": "cypress run --e2e",
    "test:component": "cypress run --component",
    "test:integration": "node -e \"process.exit(0)\"",
    "lint": "ng lint",
    "lint:staged": "eslint --fix --max-warnings=0",
    "typecheck": "tsc -p tsconfig.app.json --noEmit"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/animations": "^21.1.0",
    "@angular/common": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@dindinho/shared": "*",
    "@primeuix/themes": "^2.0.2",
    "chart.js": "^4.5.1",
    "jwt-decode": "^4.0.0",
    "libphonenumber-js": "^1.12.35",
    "marked": "^17.0.0",
    "ng2-charts": "^8.0.0",
    "ngx-markdown": "^21.1.0",
    "primeng": "^21.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.1.3",
    "@angular-devkit/core": "^21.1.3",
    "@angular-eslint/builder": "21.1.0",
    "@angular/build": "^21.1.0",
    "@angular/cli": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/compiler-cli": "^21.1.0",
    "@angular/platform-browser-dynamic": "^21.1.0",
    "@cypress/angular": "^4.1.0",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "^0.5.19",
    "@types/prismjs": "^1.26.5",
    "angular-eslint": "21.1.0",
    "cypress": "^15.10.0",
    "eslint": "^9.39.1",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.3",
    "prismjs": "^1.29.0",
    "tailwindcss": "^4.1.12",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.47.0",
    "vitest": "^4.0.8"
  },
  "overrides": {
    "undici": "7.18.2"
  }
}
